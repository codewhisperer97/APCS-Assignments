public class Year 
{	
	private int year;
	
	public Year(int year)
	{
		this.year = year;
	}
	
	public boolean isLeapYear()
	{
		if((year % 4) == 0)
		{
			System.out.println("Year % 4 == 0");
			if((year % 100) != 0)
			{
				System.out.println("Year % 100 != 0");
				return true;
			}
		}
		return false;
	}
	
	public int getYear()
	{
		return year;
	}
	
	public void setYear(int year)
	{
		this.year = year;
	}
}
